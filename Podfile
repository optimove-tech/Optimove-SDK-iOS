# platform :ios, '9.0'

target 'NotificationService' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for NotificationService
    pod 'KumulosSdkSwiftExtension', '~> 9.2'

end

target 'Optimobile' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Optimobile
    pod 'KumulosSdkSwift', '~> 9.2'
    pod 'OptimoveCore'
    pod 'OptimoveSDK'

end

target 'OptimoveNotificationExtension' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for OptimoveNotificationExtension
    pod 'OptimoveCore'
    pod 'OptimoveNotificationServiceExtension'

end
